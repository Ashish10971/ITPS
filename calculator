//calculator
#include<stdio.h>
int main()
{
    float a,b,c;
    char ch;
    printf("enter your first number\n");
    scanf("%f",&a);
    printf("enter your choice to perform operation(+,*,-,/)\n");
    scanf(" %c",&ch);
    printf("enter second number\n");
    scanf("%f",&b);
    
    switch(ch)
    {
        case '+': 
        c=a+b;
        printf("output==%-.f",c);
        break;
        case'-': 
        c=a-b;
        printf("output==%f",c);
        break;
        case '*': c=a*b;
        printf("output=%f",c);
        break;
        case '/': c=a/b;
        printf("output=%f",c);
    }
        return 0;
    }
