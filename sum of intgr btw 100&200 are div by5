#include <stdio.h>
//sum of intgr btw 100 &200 are div by 5
int main() {
    int sum = 0;

  
    for (int i = 101; i < 200; ++i) {
        
        if (i % 5 == 0) {
            sum += i;
        }
    }

  
    printf("Sum of integers greater than 100 and less than 200, divisible by 5: %d\n", sum);

    return 0;
}
